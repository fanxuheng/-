<view><view class="VerticalBox" style="margin-top:40rpx;"><scroll-view class="VerticalNav nav" style="height:calc(100vh - 175rpx);" scroll-y scroll-with-animation scroll-top="{{verticalNavTop}}"><block a:for="{{departmentList}}" a:for-item="item" a:for-index="index" a:key="id"><view class="{{(('cu-item')+' '+(item.id==tabCur?'visited-color':''))}}" data-id="{{item.id}}" data-name="{{item.name}}" data-event-opts="{{[['tap',[['TabSelect',['$event']]]]]}}" onTap="__e">{{''+item.name+''}}</view></block></scroll-view><scroll-view class="VerticalMain" style="height:calc(100vh - 175rpx);" scroll-y scroll-with-animation scroll-into-view="{{'main-'+mainCur}}" data-event-opts="{{[['scroll',[['VerticalMain',['$event']]]]]}}" onScroll="__e"><view class="padding-top padding-lr"><view class="cu-bar solid-bottom bg-white"><view class="action"><text class="cuIcon-title visited-color"></text>{{''+currentDep}}</view></view><view class="cu-list menu-avatar"><block a:for="{{outpatientList}}" a:for-item="item" a:for-index="index" a:key="id"><view data-event-opts="{{[['tap',[['toPage',['$0'],[[['outpatientList','id',item.id,'id']]]]]]]}}" class="cu-item" onTap="__e"><text class="text-position">{{item.name}}</text></view></block></view></view></scroll-view></view></view>