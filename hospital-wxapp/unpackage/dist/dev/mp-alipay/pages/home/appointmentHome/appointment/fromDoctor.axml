<view><view class="weui-search-bar"><view class="weui-search-bar__form"><view class="weui-search-bar__box"><icon class="weui-icon-search_in-box" type="search" size="14" data-event-opts="{{[['tap',[['getDoctorList']]]]}}" onTap="__e"></icon><input class="weui-search-bar__input" type="text" placeholder="请输入医生姓名" confirm-type="search" data-event-opts="{{[['confirm',[['getDoctorList']]],['input',[['__set_model',['','inputVal','$event',[]]]]]]}}" value="{{inputVal}}" onConfirm="__e" onInput="__e"/><block a:if="{{inputVal.length>0}}"><view data-event-opts="{{[['tap',[['clearInput',['$event']]]]]}}" class="weui-icon-clear" onTap="__e"><icon type="clear" size="14"></icon></view></block></view></view></view><block a:for="{{doctorList}}" a:for-item="item" a:for-index="index" a:key="id"><view data-event-opts="{{[['tap',[['toDoctorBrief',['$0'],[[['doctorList','id',item.id]]]]]]]}}" class="doctor-outbox" style="margin-top:60rpx;" onTap="__e"><image class="doctor-icon" src="{{item.gender===1?iconURL+'man-doctor.png':iconURL+'women-doctor.png'}}"></image><view class="doctor-info"><text class="doctor-name">{{item.name}}</text><view class="doctor-job"><text class="blue-text">{{item.specialName+"/"+item.outpatientName}}</text><text class="gray-text">{{item.jobTitle}}</text></view><text class="gray-text cut-long-text">{{"简介："+item.specialty}}</text></view></view></block></view>