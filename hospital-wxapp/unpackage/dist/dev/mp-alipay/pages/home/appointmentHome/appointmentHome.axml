<view><block a:if="{{insureVisited==0}}"><from-hosipital vue-id="1" onVueInit="__l"></from-hosipital></block><block a:if="{{insureVisited==1}}"><from-doctor vue-id="2" onVueInit="__l"></from-doctor></block><block a:if="{{insureVisited==2}}"><from-date vue-id="3" onVueInit="__l"></from-date></block><block a:if="{{insureVisited==3}}"><from-department vue-id="4" onVueInit="__l"></from-department></block><image class="icon-up-position" src="/static/appointment/icon-up.png" data-event-opts="{{[['tap',[['showModal']]]]}}" onTap="__e"></image><view class="{{(('cu-modal bottom-modal')+' '+(isShowModal?'show':''))}}"><view class="cu-dialog"><view class="cu-bar bg-white"><view data-event-opts="{{[['tap',[['changeFrom',['$event']]]]]}}" class="action text-green" onTap="__e">确定</view><view data-event-opts="{{[['tap',[['hideModal']]]]}}" class="action text-blue" onTap="__e">取消</view></view><view class="change-appoint-form"><block a:for="{{modalData}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['selectFrom',[index]]]]]}}" class="appoint-inbox" onTap="__e"><block a:if="{{modalVisited==index}}"><view data-name="{{index}}"><view class="round-icon-box visited-round"><image class="icon" src="{{imgUrl+item.visited}}"></image></view><text class="visited-text">{{item.text}}</text></view></block><block a:else><view data-name="{{index}}"><view class="round-icon-box invisited-round"><image class="icon" src="{{imgUrl+item.invisited}}"></image></view><text class="invisited-text">{{item.text}}</text></view></block></view></block></view></view></view></view>