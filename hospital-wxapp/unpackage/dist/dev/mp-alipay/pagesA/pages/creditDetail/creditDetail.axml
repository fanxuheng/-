<view class="weui-tab body"><view class="weui-navbar"><block a:for="{{tabs}}" a:for-item="item" a:for-index="index" a:key="index"><block><view class="{{(('weui-navbar__item')+' '+(activeIndex==index?'weui-bar__item_on nav-bottom-border':''))}}" id="{{index}}" data-event-opts="{{[['tap',[['tabClick',['$event']]]]]}}" onTap="__e"><view class="weui-navbar__title">{{item}}</view></view></block></block></view><view class="weui-tab__panel"><view class="weui-tab__content" hidden="{{activeIndex!=0}}"><credit-page hidden="{{!(activeIndex==0)}}" vue-id="1" ref="__r" data-ref="creditPage" onVueInit="__l"></credit-page></view><view class="weui-tab__content" hidden="{{activeIndex!=1}}"><history-credit hidden="{{!(!isCreditGood)}}" vue-id="2" ref="__r" data-ref="historyCredit" onVueInit="__l"></history-credit></view></view><image class="icon-up-position" src="/static/appointment/icon-up.png" hidden="{{activeIndex!=1}}" data-event-opts="{{[['tap',[['showModal']]]]}}" onTap="__e"></image><view class="{{(('cu-modal bottom-modal')+' '+(isShowModal?'show':''))}}"><view class="cu-dialog"><view class="cu-bar bg-white"><view data-event-opts="{{[['tap',[['changeCardId']]]]}}" class="action text-green" onTap="__e">确定</view><view data-event-opts="{{[['tap',[['hideModal']]]]}}" class="action text-blue" onTap="__e">取消</view></view><view class="bottom-hospital-box"><block a:for="{{cardList}}" a:for-item="item" a:for-index="index" a:key="id"><view class="hospital-box"><view data-event-opts="{{[['tap',[['selectCardId',['$0'],[[['cardList','id',item.id,'id']]]]]]]}}" class="{{(('hospital')+' '+(modalVisited==item.id?'visited-background':'invisited-background'))}}" onTap="__e"><text class="text">{{''+item.name+''}}</text></view></view></block></view></view></view></view>