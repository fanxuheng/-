<view><view class="top-select-date"><block a:for="{{dateList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{index<3}}"><view data-event-opts="{{[['tap',[['changeDate',[index]]]]]}}" class="box" onTap="__e"><text class="{{(('day')+' '+(visitedIndex==index?'visited-color':''))}}">{{item.day}}</text><block a:if="{{index==0}}"><text class="{{(('date')+' '+(visitedIndex==index?'visited-color':''))}}">今</text></block><block a:else><view><text class="{{(('date')+' '+(visitedIndex==index?'visited-color':''))}}">{{item.date}}</text></view></block></view></block></block><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="box" onTap="__e"><image class="icon" src="{{isShowMoreDate?'/static/appointment/icon-up.png':'/static/appointment/icon-down.png'}}"></image><block a:if="{{isShowMoreDate}}"><text class="day">收起</text></block><block a:if="{{!isShowMoreDate}}"><text class="day">展示</text></block></view></view><transition vue-id="1" name="slide-fade" onVueInit="__l"><block a:if="{{isShowMoreDate}}"><view class="top-select-date"><block a:for="{{dateList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{index>=3}}"><view data-event-opts="{{[['tap',[['changeDate',[index]]]]]}}" class="box" onTap="__e"><text class="{{(('day')+' '+(visitedIndex==index?'visited-color':''))}}">{{item.day}}</text><text class="{{(('date')+' '+(visitedIndex==index?'visited-color':''))}}">{{item.date}}</text></view></block></block></view></block></transition><view class="tips-positon"><view data-event-opts="{{[['tap',[['showModal']]]]}}" class="tips-inbox" onTap="__e"><image class="icon" src="/static/appointment/tips.png"></image><text class="tips-text">您可以点击<text style="color:#7EC0EE;">这里</text>切换不同分院</text></view></view><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="id"><block a:if="{{isGetData}}"><view data-event-opts="{{[['tap',[['toDoctorAppointDetail',['$0'],[[['doctorList','doctorInfo.id',item.$orig.doctorInfo.id]]]]]]]}}" class="doctor-outbox" onTap="__e"><image class="doctor-icon" src="{{item.$orig.doctorInfo.gender===1?iconURL+'man-doctor.png':iconURL+'women-doctor.png'}}"></image><view class="doctor-info"><view class="doctor-job-box"><text class="doctor-name">{{item.$orig.doctorInfo.name}}</text><text class="gray-text">{{item.$orig.doctorInfo.jobTitle}}</text></view><text class="gray-text ">出诊：<text style="color:#7EC0EE;">{{item.f0}}</text></text></view></view></block></block><block a:if="{{!isGetData}}"><no-data-page vue-id="2" message="{{message}}" onVueInit="__l"></no-data-page></block><view class="{{(('cu-modal bottom-modal')+' '+(isShowModal?'show':''))}}"><view class="cu-dialog"><view class="cu-bar bg-white"><view data-event-opts="{{[['tap',[['insureChangeHospital']]]]}}" class="action text-green" onTap="__e">确定</view><view data-event-opts="{{[['tap',[['hideModal']]]]}}" class="action text-blue" onTap="__e">取消</view></view><view class="bottom-hospital-box"><block a:for="{{hospitalList}}" a:for-item="item" a:for-index="index" a:key="id"><view class="hospital-box"><view data-event-opts="{{[['tap',[['changeHospital',['$0','$1'],[[['hospitalList','id',item.id,'id']],[['hospitalList','id',item.id,'name']]]]]]]}}" class="{{(('hospital')+' '+(showVisited==item.id?'visited-background':'invisited-background'))}}" onTap="__e"><text class="text">{{''+item.name+''}}</text></view></view></block></view></view></view></view>