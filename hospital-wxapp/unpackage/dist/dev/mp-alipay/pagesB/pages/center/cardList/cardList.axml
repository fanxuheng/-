<view><text class="gray-tips">最多可添加5个就诊卡</text><block a:if="{{isGetData}}"><view class="main"><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="relationId"><view class="main_item"><movable-area><movable-view damping="100" out-of-bounds="{{true}}" direction="horizontal" x="{{show==item.$orig.relationId?x:''}}" animation="{{false}}" data-name="{{item.$orig.relationId}}" data-event-opts="{{[['change',[['test',['$event']]]],['touchEnd',[['touchend',['$event']]]]]}}" onChange="__e" onTouchEnd="__e"><view class="info-box"><view class="row-box" style="margin-top:20rpx;"><text class="name">{{item.$orig.name}}</text><image class="icon" src="{{item.$orig.gerder==1?iconPath+'male.png':iconPath+'female.png'}}"></image></view><view class="row-box"><text class="gray-text">卡号：</text><text class="gray-text">{{item.$orig.id}}</text></view><view class="row-box"><text class="gray-text">身份证号：</text><text class="gray-text">{{item.f0}}</text></view></view></movable-view></movable-area><block a:if="{{show==item.$orig.relationId}}"><view class="delete_box"><view class="round-outbox"><view data-event-opts="{{[['tap',[['handleEdit',['$0'],[[['cardList','relationId',item.$orig.relationId]]]]]]]}}" class="{{(('round-box')+' '+(handle==1?'blue-background':''))}}" onTap="__e"><image class="icon" src="/static/appointment/edit.png"></image></view></view><view class="round-outbox"><view data-event-opts="{{[['tap',[['openConfirm',['$0','$1'],[[['cardList','relationId',item.$orig.relationId,'name']],'show']]]]]}}" class="{{(('round-box')+' '+(handle==2?'red-background':''))}}" onTap="__e"><image class="icon" src="/static/appointment/delete.png"></image></view></view></view></block></view></block></view></block><block a:if="{{!isGetData}}"><no-data-page vue-id="1" message="{{message}}" onVueInit="__l"></no-data-page></block><image class="add-button" src="/static/appointment/add.png" data-event-opts="{{[['tap',[['toAddCard']]]]}}" onTap="__e"></image></view>