<viem vue-id="1" onVueInit="__l"><view class="top-title">{{"发送验证码至"+$root.f0}}</view><view class="cu-form-group code-box"><view class="title">验证码</view><input placeholder="请输入验证码" name="input" data-event-opts="{{[['input',[['__set_model',['','phoneNum','$event',[]]]]]]}}" value="{{phoneNum}}" onInput="__e"/><block a:if="{{codeNum===0}}"><button data-event-opts="{{[['tap',[['getPhoneCode']]]]}}" hidden="{{!(!isGetCode)}}" class="cu-btn bg-green shadow" onTap="__e">获取验证码</button></block><block a:else><button class="cu-btn bg-gray shadow">{{"再次获取"+codeNum+"s"}}</button></block></view><button data-event-opts="{{[['tap',[['validataCode']]]]}}" class="button-box" onTap="__e">确定</button></viem>