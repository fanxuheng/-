<view><block a:if="{{isAuthrization}}"><view class="input-inbox"><input class="input" placeholder-class="placeholder-class" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" onInput="__e"/></view></block><block a:if="{{isAuthrization}}"><view class="password-row-box"><input class="input" password="{{!isVisible}}" placeholder-class="placeholder-class" placeholder="请输入登录密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" onInput="__e"/><image class="eye-icon" src="{{isVisible?'/static/login/eye.png':'/static/login/eye-off.png'}}" data-event-opts="{{[['tap',[['changeVisible']]]]}}" onTap="__e"></image></view></block><block a:if="{{!isAuthrization}}"><button class="button" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo']]],['tap',[['getUserInfo']]]]}}" onGetuserinfo="__e" onTap="__e">授权登录</button></block><block a:else><button data-event-opts="{{[['tap',[['toPageCenter']]]]}}" class="button" onTap="__e">登录</button></block><block a:if="{{isAuthrization}}"><view class="enroll-changepassword-box"><text data-event-opts="{{[['tap',[['toRegister']]]]}}" class="{{(('left')+' '+(visited==1?'visited-color':''))}}" onTap="__e">立即注册</text><text data-event-opts="{{[['tap',[['toForgotPassword']]]]}}" class="{{(('right')+' '+(visited==2?'visited-color':''))}}" onTap="__e">忘记密码</text></view></block></view>